<%= 
  @student_applications.each do |app|     
    if app.final_decision.admitted == true
      @decision = "accept"
    else
      @decision = "reject"
    end
  end
%>
<%- headers = ['Name', 'Email', 'Semester', 'Year', 'Decision'] -%>
<%= CSV.generate_line headers %>
<%- @student_applications.each do |app| -%>
  <%= CSV.generate_line([app.applicant.name, app.applicant.email, app.semester.season, app.semester.year, @decision]) %>
<%- end -%>