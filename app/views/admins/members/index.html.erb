<h1><%= link_to "New Member", new_admin_member_path, method: :get %></h1>

<table border="1">
  <% @members.each do |member| %>
    <tr>
      <td><%= member.last_name %></td>
      <td><%= member.first_name %></td>
      <td><%= member.email %></td>
      <td><%= button_to "View", admin_member_path(member), method: :get %></td>
      <td><%= button_to "Edit", edit_admin_member_path(member), method: :get %></td>
      <td><%= button_to "Delete", admin_member_path(member), method: :delete %></td>
    </tr>
  <% end %>
</table>
