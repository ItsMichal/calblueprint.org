<%= simple_form_for [:admin, @member], url: { action: "update" } do |f| %>
  <%= f.input :first_name, label: 'First name:', required: true %><br />
  <%= f.input :last_name, label: 'Last name:', required: true %><br />
  <%= f.input :email, label: 'Email:', required: true %><br />
  <%= f.submit 'Save Changes' %>
<% end %>
