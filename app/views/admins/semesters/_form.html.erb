<%= simple_form_for [:admin, @semester] do |f| %>
  <%= f.input :season, label: 'Season:' %><br />
  <%= f.input :year, label: 'Year:' %><br />
  <%= f.input :is_current_semester, as: :boolean, label: 'Current Semester:', input_html: { id: "is_current_semester" } %><br />
  <%= f.button :submit %>
<% end %>
