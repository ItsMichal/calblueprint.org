<h1><%= @semester.season %></h1>
<h1><%= @semester.year %></h1>

<h1>Apps</h1>
<table border="1">
  <% @semester.apps.each do |app| %>
    <tr>
      <td><%= app.applicant.name %></td>
      <td><%= app.applicant.email %></td>
      <td><%= app.year %></td>
    </tr>
  <% end %>
</table>

