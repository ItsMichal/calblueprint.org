<div class="header">
  <%= link_to "Apply", apply_path, class: "header-tab header-button" %>

  <% if !current_user %>
    <!-- Temporary log in link -->
    <%= link_to "Log in", new_admin_session_path, class: "header-tab" %>
  <% else %>
    <%= link_to "Log out", destroy_user_session_path, class: "header-tab", method: :delete %>
  <% end %>
  <%= link_to "Contact", new_contact_form_path, class: "header-tab" %>
  <%= link_to "Blog", '#', class: "header-tab" %>
  <%= link_to "Projects", projects_path, class: "header-tab" %>
  <%= link_to "About", about_path, class: "header-tab" %>
  <% if current_admin %>
    <%= link_to "Applications", admin_apps_path, class: "header-tab" %>
  <% end %>
  <%= link_to "", root_path, class: "header-logo" %>
</div>
